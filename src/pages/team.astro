---
/**
 * /team - Team Page with Person Schema
 * 
 * SEO Optimizations:
 * - Person schema for each team member (Google Knowledge Panel potential)
 * - ItemList schema for team listing
 * - BreadcrumbList for navigation
 * - Optimized meta for "DevUp Society team" queries
 * 
 * TARGET KEYWORDS:
 * - DevUp Society team
 * - DevUp Society leadership
 * - student developer community leaders
 * - VJIT coding club team
 */
import MainLayout from '../layouts/MainLayout.astro';
import { coreTeam } from '../data/team';

// Faculty Advisors - 3 column grid
const principal = coreTeam.find(m => m.id === 'tm-001');
const obulesu = coreTeam.find(m => m.id === 'tm-002');
const ratnakar = coreTeam.find(m => m.id === 'tm-003');
const facultyAdvisors = [principal, obulesu, ratnakar].filter(Boolean);

// Faizan and Asif - 2 person grid
const faizan = coreTeam.find(m => m.id === 'tm-010');
const asif = coreTeam.find(m => m.id === 'tm-011');
const topLeaders = [faizan, asif].filter(Boolean);

// Special 3-person grid: Thapendra, Narsing, Sanchit
const specialThree = [
  coreTeam.find(m => m.id === 'tm-012'), // Thapendra
  coreTeam.find(m => m.id === 'tm-013'), // Narsing
  coreTeam.find(m => m.id === 'tm-014'), // Sanchit
].filter(Boolean);

// Other core team members (rest of the leads)
const otherLeads = coreTeam.filter(m => 
  m.id !== 'tm-010' && // Faizan
  m.id !== 'tm-001' && // Principal
  m.id !== 'tm-002' && // Obulesu
  m.id !== 'tm-003' && // Ratnakar
  m.id !== 'tm-011' && // Asif
  m.id !== 'tm-012' && // Thapendra
  m.id !== 'tm-013' && // Narsing
  m.id !== 'tm-014'    // Sanchit
);

// Generate Person schemas for all team members
const generatePersonSchemas = () => {
  return coreTeam.map(member => ({
    "@context": "https://schema.org",
    "@type": "Person",
    "name": member.name,
    "jobTitle": member.role,
    "image": member.photo || undefined,
    "worksFor": {
      "@type": "Organization",
      "name": "DevUp Society",
      "url": "https://www.devupvjit.in"
    },
    "memberOf": {
      "@type": "Organization",
      "name": "DevUp Society"
    },
    "sameAs": [
      member.socialLinks?.linkedin,
      member.socialLinks?.github,
      member.socialLinks?.instagram,
      member.socialLinks?.twitter,
      member.socialLinks?.portfolio
    ].filter(Boolean),
    "description": member.quote || `${member.role} at DevUp Society`
  }));
};

// Generate ItemList schema for the team page
const teamListSchema = {
  "@context": "https://schema.org",
  "@type": "ItemList",
  "name": "DevUp Society Team Members",
  "description": "Leadership team and core members of DevUp Society - India's premier student developer community",
  "numberOfItems": coreTeam.length,
  "itemListElement": coreTeam.map((member, index) => ({
    "@type": "ListItem",
    "position": index + 1,
    "item": {
      "@type": "Person",
      "name": member.name,
      "jobTitle": member.role,
      "image": member.photo
    }
  }))
};

// Breadcrumb schema
const breadcrumbSchema = {
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    { "@type": "ListItem", "position": 1, "name": "Home", "item": "https://www.devupvjit.in" },
    { "@type": "ListItem", "position": 2, "name": "Team", "item": "https://www.devupvjit.in/team" }
  ]
};

// Combine schemas
const combinedSchemas = [teamListSchema, breadcrumbSchema];
