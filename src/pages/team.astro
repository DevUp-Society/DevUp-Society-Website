                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M22 4s-.7 2.1-2 3.4c1.6 10-9.4 17.3-18 11.6 2.2.1 4.4-.6 6-2C3 15.5.5 9.6 3 5c2.2 2.6 5.6 4.1 9 4-.9-4.2 4-6.6 7-3.8 1.1 0 3-1.2 3-1.2z"/></svg>
                      </a>
                    )}
                    {member.socialLinks.portfolio && (
                      <a href={member.socialLinks.portfolio} target="_blank" rel="noopener noreferrer" class="text-zinc-600 hover:text-signal transition-colors">
                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"/><path d="M12 2a14.5 14.5 0 0 0 0 20 14.5 14.5 0 0 0 0-20"/><path d="M2 12h20"/></svg>
                      </a>
                    )}
                  </div>
                )}
                
                <!-- View Team Button -->
                {!['tm-011', 'tm-012', 'tm-013', 'tm-014', 'tm-029'].includes(member.id) && (
                  <a 
                    href={`/teams/${member.teamSlug}`}
                    class="mt-auto w-full bg-zinc-900 hover:bg-signal text-white hover:text-black font-mono text-xs font-bold tracking-widest uppercase py-3 px-4 transition-all text-center border border-zinc-800 hover:border-signal"
                  >
                    View {member.teamName}
                  </a>
                )}
              </div>
            </div>
          ))}
        </div>
      )}
    </div>
  </section>

  <!-- CTA SECTION -->
  <section class="py-32 bg-[#050505] border-t border-zinc-900">
    <div class="max-w-4xl mx-auto px-6 text-center">
      <div class="glass-panel p-12 clip-corner">
        <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="text-signal mb-6"><path d="M16 21v-2a4 4 0 0 0-4-4H6a4 4 0 0 0-4 4v2"/><circle cx="9" cy="7" r="4"/><path d="M22 21v-2a4 4 0 0 0-3-3.87"/><path d="M16 3.13a4 4 0 0 1 0 7.75"/></svg>
        <h2 class="font-display text-5xl text-white mb-6 leading-tight">
          Want to Join<br/>the Core Team?
        </h2>
        <p class="font-mono text-zinc-400 text-sm mb-8 max-w-lg mx-auto">
          We're always looking for passionate builders. If you have the skills and the drive, we want to hear from you. 
          For organizational or partnership inquiries, reach us at <a href="mailto:devupsociety@vjit.ac.in" class="text-signal hover:text-white transition-colors">devupsociety@vjit.ac.in</a>.
        </p>
        <a href="/join" class="inline-flex items-center gap-3 px-8 py-4 bg-signal text-black font-mono text-xs font-bold tracking-widest uppercase hover:bg-white transition-colors clip-corner btn-glitch">
          <span class="relative z-10">Apply Now</span>
          <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="relative z-10"><path d="M5 12h14"/><path d="m12 5 7 7-7 7"/></svg>
        </a>
      </div>
    </div>
  </section>
</MainLayout>
